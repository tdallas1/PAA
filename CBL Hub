    <!-- Back to Top Button -->
    <a href="#" class="back-to-top" id="back-to-top">
        <i class="fas fa-arrow-up"></i>
    </a>

    <!-- JavaScript -->
    <script>
        // Initialize AOS (Animate On Scroll)
        document.addEventListener('DOMContentLoaded', function() {
            // Initialize AOS
            AOS.init({
                duration: 800,
                easing: 'ease-in-out',
                once: true
            });
            
            // Progress Bar Animation for Steps
            const progressBar = document.getElementById('progress-bar');
            const stepItems = document.querySelectorAll('.step-item');
            
            let currentStep = 0;
            const interval = setInterval(() => {
                if (currentStep < stepItems.length) {
                    const progress = (currentStep + 1) / stepItems.length * 100;
                    progressBar.style.width = `${progress}%`;
                    
                    // Add active class to the current step
                    stepItems.forEach((item, index) => {
                        if (index <= currentStep) {
                            item.classList.add('active');
                        } else {
                            item.classList.remove('active');
                        }
                    });
                    
                    currentStep++;
                } else {
                    currentStep = 0;
                }
            }, 2000);
            
            // FAQ Accordion
            const accordionItems = document.querySelectorAll('.accordion-item');
            
            accordionItems.forEach(item => {
                const mainHeader = item.querySelector('.accordion-mainHeader');
                mainHeader.addEventListener('click', () => {
                    item.classList.toggle('active');
                });
            });
            
            // Back to Top Button
            const backToTopBtn = document.getElementById('back-to-top');
            
            window.addEventListener('scroll', () => {
                if (window.pageYOffset > 300) {
                    backToTopBtn.classList.add('visible');
                } else {
                    backToTopBtn.classList.remove('visible');
                }
            });
            
            backToTopBtn.addEventListener('click', (e) => {
                e.preventDefault();
                window.scrollTo({
                    top: 0,
                    behavior: 'smooth'
                });
            });
            
            // Sticky mainHeader
            const mainmainHeader = document.getElementById('main-mainHeader');
            const quickNav = document.getElementById('quick-nav');
            const heroHeight = document.getElementById('hero').offsetHeight;
            
            window.addEventListener('scroll', () => {
                if (window.pageYOffset > 100) {
                    mainHeader.classList.add('scrolled');
                } else {
                    mainHeader.classList.remove('scrolled');
                }
                
                if (window.pageYOffset > heroHeight) {
                    quickNav.classList.add('visible');
                } else {
                    quickNav.classList.remove('visible');
                }
            });
            
            // Smooth Scrolling for Nav Links
            const navLinks = document.querySelectorAll('.quick-nav-link');
            
            navLinks.forEach(link => {
                link.addEventListener('click', (e) => {
                    e.preventDefault();
                    
                    const targetId = link.getAttribute('href');
                    const targetElement = document.querySelector(targetId);
                    
                    const offsetTop = targetElement.offsetTop - 100;
                    
                    window.scrollTo({
                        top: offsetTop,
                        behavior: 'smooth'
                    });
                });
            });
        });
    </script>
</body>
</html>
